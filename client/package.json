{
  "name": "pumpbump-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run pre-deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "pre-deploy": "node ./src/scripts/pre-deploy.js",
    "analyze": "cross-env ANALYZE=true next build",
    "supabase:serve": "supabase functions serve --env-file ./supabase/functions/.env.local",
    "supabase:gen-types": "supabase gen types typescript --local > ./src/lib/supabase/database.ts",
    "supabase:reset": "supabase db reset && npm run supabase:gen-types",
    "supabase:dump": "supabase db dump --data-only --local -f ./supabase/dump.sql",
    "git:reset": "git stash && git checkout main && git pull && git branch -D staging && git checkout -b staging && git push -uf origin staging && git stash pop"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.6.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@sentry/nextjs": "^8.9.2",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.5",
    "@tanstack/react-query": "^5.45.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "jotai": "^2.8.3",
    "lucide-react": "^0.395.0",
    "next": "14.2.4",
    "nprogress": "^0.2.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.52.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.4",
    "@tanstack/react-query-devtools": "^5.45.1",
    "@types/node": "^20",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "openapi-fetch": "^0.10.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.2"
  }
}
